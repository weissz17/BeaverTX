{
  "manifest_version": 2,

  "name": "BeaverTCH",
  "description": "An extension for Beaver Teachers",
  "version": "0.0.1",

  "browser_action": {
   "default_icon": "hu.png",
   "default_popup": "popup.html"
  },

  "options_page": "options.html",



  "permissions": [
   "activeTab",
   "storage" ,
   "contextMenus",
   "alarms",
   "notifications"
 ],

 "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",


  "icons":
  {
     "128": "icon128tr.png",
     "1024": "icon1024tr.png",
      "16": "icon16tr.png"
  }

}
